<MenuBar>
	<Menu name="Chess">
		<Menu name="About Chess" shell="win -a"/>
		<Menu type="divider"/>
		<Menu name="Quit Chess" shell="win -c"/>
	</Menu>
	<Menu name="Game">
		<Menu name="New Game" click="new-game" hotkey="alt+n"/>
		<Menu type="divider"/>
		<Menu name="Rotate Board" click="rotate-board" hotkey="alt+r"/>
		<Menu type="divider"/>
		<Menu name="Theme&#8230;">
			<Menu name="Blue" click="set-board-theme" arg="blue" check-group="board-theme"/>
			<Menu name="Brown" click="set-board-theme" arg="brown" check-group="board-theme"/>
		</Menu>
	</Menu>
	<Menu name="Help">
		<Menu name="Help" click="open-help"/>
		<Menu name="Stockfish Engine Interface" click="engine-interface"/>
		<Menu type="divider"/>
		<Menu name="Source Code"/>
	</Menu>
</MenuBar>

